<?xml version="1.0"?>
<object name="portal_skins" default_skin="GenwebAlternaTheme">
 <object name="genwebtheme_images"
    meta_type="Filesystem Directory View"
    directory="genweb.alternatheme:skins/genwebtheme_images"/>
 <object name="genwebtheme_custom"
    meta_type="Filesystem Directory View"
    directory="genweb.alternatheme:skins/genwebtheme_custom"/>

 <skin-path name="GenwebAlternaTheme" based-on="Plone Default">
  <layer name="genwebtheme_images" insert-after="custom"/>
  <layer name="genwebtheme_custom" insert-after="custom"/>
 </skin-path>

</object>
