<ul tal:define="results view/get_contenttags_by_query;
                portal_url context/@@plone_portal_state/portal_url;">
  <li tal:repeat="obj results">
    <div class="row">
      <div class="col-xs-12">
        <a tal:attributes="href obj/getURL">
          <div class="imgLibray" tal:condition="obj/existImage">
              <img tal:attributes="src string:${obj/getURL}/@@images/image;
                                   title obj/Title;
                                   alt obj/Title "/>
          </div>
          <div class="imgLibray" tal:condition="not:obj/existImage">
              <img tal:attributes="src string:${portal_url}/++switchmed++static/images/image_default.png "/>
          </div>
          <h4 tal:content="obj/Title">Title</h4>
        </a>
        <p tal:condition="obj/Description" tal:content="obj/Description">Description</p>
      </div>
    </div>
  </li>
</ul>